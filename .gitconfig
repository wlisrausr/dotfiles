[core]
    excludesfile = /Users/wlisrausr/.gitignore_global
    editor = vim
    autocrlf = input
    pager = vimpager
[user]
    name = Wanda Ichsanul Isra
    email = wandaisra@gmail.com
[alias]
    dsf = "!f() { [ -z \"$GIT_PREFIX\" ] || cd \"$GIT_PREFIX\" && git diff --color \"$@\" | diff-so-fancy  | less --tabs=4 -RFX; }; f"
[push]
    default = current
[color]
    ui = always
[merge]
    ff = false
[pager]
    dsf = vimpager
[credential]
    helper = cache
[help]
    autocorrect = 1
[branch]
    autosetuprebase = always
[url "git@github.com:"]
	insteadOf = https://github.com/
